{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: Train DDPM-IQL Offline",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/launcher/examples/train_ddpm_iql_offline.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/venv/bin/python",
            "args": [
                "--variant",
                "0"
            ],
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug: Train DDPM-IQL Finetune",
